<?xml version="1.0" encoding="utf-8" ?>
<Canvas name="AWSLEditorView" 
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
        xsi:noNamespaceSchemaLocation="ViewSchema.xsd">
  <Panel name="AWSLEditorPanel" RectTransform="{anchorMin:(0.5,0.5); anchorMax:(0.5,0.5); sizeDelta:(800,960);}">
    <InputField name="CodeInputField" RectTransform="{anchorMin:(0.05,0.1); anchorMax:(1,1); anchoredPosition:(0,0); sizeDelta:(0,0);}" lineType="MultiLineNewline">
      <Text fontSize="18" font="SpaceMono-Regular"/>
    </InputField>
    <Button name="SaveButton" RectTransform="{anchorMin:(0.35,0.05); anchorMax:(0.35,0.05); sizeDelta:(150,50);}" Text="Save"/>
    <Button name="CancelButton" RectTransform="{anchorMin:(0.65,0.05); anchorMax:(0.65,0.05); sizeDelta:(150,50);}" Text="Cancel"/>
    <Text name="LineNumber" RectTransform="{anchorMin:(0.0275,0); anchorMax:(0.0275,0.993); sizeDelta:(30,0);}" alignment="UpperRight" font="SpaceMono-Regular"/>
  </Panel>
</Canvas>