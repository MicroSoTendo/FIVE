<?xml version="1.0" encoding="UTF-8"?>
<Canvas name="OptionMenuView"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="ViewSchema.xsd">
    <Button name = "LoadButton" Prefab = "EntityPrefabs/MenuButton" Transform = "{localPosition:(0,100,0);}" Text = "Load"/>
    <Button name = "SaveButton" Prefab = "EntityPrefabs/MenuButton" Transform = "{localPosition:(0,0,0);}" Text = "Save"/>
    <Button name = "GameOptionButton" Prefab = "EntityPrefabs/MenuButton" Transform = "{localPosition:(0,-100,0);}" Text = "Game Options"/>
    <Button name = "SoundOptionButton" Prefab = "EntityPrefabs/MenuButton" Transform = "{localPosition:(0,-200,0);}" Text = "Sound Options"/>
    <Button name = "ExitGameButton" Prefab = "EntityPrefabs/MenuButton" Transform = "{localPosition:(-230,200,0);}" Text = "Exit"/>
    <Button name = "ResumeButton" Prefab = "EntityPrefabs/MenuButton" Transform = "{localPosition:(230,200,0);}" Text = "Back"/>
</Canvas>